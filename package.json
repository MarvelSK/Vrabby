{
  "name": "vrabby",
  "version": "1.0.0",
  "private": true,
  "description": "Vrabby coding AI agent",
  "scripts": {
    "dev": "node scripts/kill-ports.js && pnpm run ensure:env && pnpm run ensure:venv && concurrently --raw --names \"API,WEB\" -c \"auto\" \"pnpm run dev:api\" \"pnpm run dev:web\"",
    "dev:api": "node scripts/run-api.js",
    "dev:web": "node scripts/run-web.js",
    "ensure:env": "node scripts/setup-env.js",
    "ensure:venv": "node scripts/setup-venv.js",
    "postinstall": "pnpm run ensure:env && pnpm run ensure:venv",
    "setup": "pnpm install",
    "clean": "node scripts/clean.js",
    "db:reset": "node scripts/reset-db.js",
    "db:backup": "node scripts/backup-db.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  }
}
